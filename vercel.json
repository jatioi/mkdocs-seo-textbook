{
  "version": 2,
  "builds": [
    {
      "src": "mkdocs.yml",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "python3 -m pip install -r requirements.txt && python3 -m mkdocs build",
        "outputDirectory": "site"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 